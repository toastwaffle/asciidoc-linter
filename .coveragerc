# .coveragerc - Coverage configuration file
[run]
source = asciidoc_linter
omit = 
    */tests/*
    */site-packages/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    def main\(\):

[html]
directory = htmlcov
title = AsciiDoc Linter Coverage Report